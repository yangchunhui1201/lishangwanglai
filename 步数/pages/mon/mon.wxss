.ban_box{
  width: 100%;
  height: 218rpx;
  background-image: url(data:image/png;data:image/png;base64,);
  background-size:100% 100%; 
  position: relative;
}
.ban_box text{
  font-size: 24rpx;
  border-bottom: 1px solid white;
  color: white;
  position: absolute;
  background: #fc651e;
  bottom: 24rpx;
  right: 30rpx;
}
.cont_box{
  height: 100vh;
  background: linear-gradient(to bottom, #fd6921,#fe8333)
}
.cont_tit_box{
   width: 500rpx;
   height: 62rpx;
    background: linear-gradient(to bottom, #8c3bb1,#af61d8);
    margin:0 auto;
    border-radius: 28rpx;
    color:white;
    font-size: 23rpx;
    line-height: 62rpx;
    margin-bottom: 30rpx;
    text-align: center;
}
.container-out {
  height: 600rpx;
  width: 650rpx;
  background-color: #b136b9;
  margin: 100rpx auto;
  border-radius: 40rpx;
  box-shadow: 0 10px 0 #871a8e;
  position: relative;
  margin-top:10rpx;
}

.container-in {
  width: 580rpx;
  height: 530rpx;
  background-color: #871a8e;
  border-radius: 40rpx;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
}

/**
box-shadow: inset 3px 3px 3px #fff2af;*/

.circle {
  position: absolute;
  display: block;
  border-radius: 50%;
  height: 20rpx;
  width: 20rpx;
}

.content-out {
  position: absolute;
  height: 150rpx;
  width: 166.6666rpx;
  background: white;
  border-radius: 15rpx;
  box-shadow: 0 5px 0 #d87fde;
}

/** */

.start-btn {
  position: absolute;
  margin: auto;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  border-radius: 15rpx;
  height: 150rpx;
  width: 166.6666rpx;
  background-color: #ffe400;
  box-shadow: 0 5px 0 #e7930a;
  color: #f6251e;
  text-align: center;
  font-size: 55rpx;
  font-weight: bolder;
  line-height: 150rpx;
}

.award-image {
  position: absolute;
  margin: auto;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  height: 140rpx;
  width: 130rpx;
}
.bot_box{
  width: 440rpx;
  height: 127rpx;
  border-bottom-left-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
  background: #ffe9b0;
  position: absolute;
  bottom:80rpx;
  left:150rpx;
}